<div class="btn-group" role="group" aria-label="Button group">
  <%= link_to_back(url: backend_assets_path) if member_action? || create_actions? %>
  <%= link_to_new(url: new_backend_asset_path) if collection_action? || show_action? || update_actions? %>
  <%= link_to_edit(url: edit_backend_asset_path(@asset)) if show_action? %>
  <%= link_to_delete(url: backend_asset_path(@asset)) if show_action? || update_actions? %>
</div>
